<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ssma.serverapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCI97SxVIVB1-KL-sY1LKXyMvs2U3zefUM" />

        <activity android:name=".LoginActivity" />
        <activity android:name=".NonMembers.NonMemberFirstActivity" />
        <activity android:name=".NonMemberGoogleMap.NonMemberGoogleMap" />
        <activity android:name=".FirstActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".Chat.MessageActivity" />

        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".Fragments.SelectPeopleActivity" />
        <activity android:name=".Chat.GroupMessageActivity" />
    </application>

</manifest>